page{
  background-color: #eeeeee;
}
.icon{
  width: 100%;
  height: 140px;
  position: relative;
}

.icon_bg{
  display: block;
  height: 100%;
  /* -webkit-filter: blur(5rpx);filter: blur(8rpx);opacity: .5; */
}

.icon_header{
  position: absolute;
  top: 30px;
  width: 60px;
  height: 60px;
  border: 1px;
  border-color: lightgray;
  border-radius: 30px;
  border-style: solid;
  left:50%;
  transform:translateX(-50%);
}

.icon text{
  position: absolute;
  font-size: 14px;
  color: gray;
  top: 100px;
  left:50%;
  transform:translateX(-50%);
}
.list_item{
  width: 100%;
  height: 40px;
  display: flex;
  background-color: white;
  line-height: 40px;
  justify-content:space-between;
  margin-top: 1px;
}

.list_item image{
  width: 20px;
  height: 20px;
  padding-top: 10px;
  padding-right: 5px;
}

.list_item text{
  padding-left: 5px;
  font-size: 14px;
}
