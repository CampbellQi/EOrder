
page{
  width: 100%;
  height: 100%;
  display: flex;
}
.desk_num_list{
  width: 80px;
  height: 100%;
  background-color: lightgray;
}
.desk_num_list button{
  height: 35px;
  line-height: 35px;
  font-size: 14px;
}

.order_list{
  height: 100%;
  width: calc(100% - 80px);
}

.order_list_item{
  display: flex;
  background-color: lightgray;
  flex-wrap:wrap;
}
.order_list button{
  height: 25px;
  line-height: 25px;
  font-size: 12px;
  margin-left:0;
  margin-right:0;
}

